/* bottom.wxss */

.bottom-tag{
  position:fixed;
  bottom:0px;
  background-color:#f7f6fb;
  width:100%;
  height:90rpx
}

.left-tag{
  display:inline-block;
  margin-left:100rpx;
  margin-top:4rpx
}

.right-tag{
  float:right;
  margin-right:100rpx;
  margin-top:4rpx
}

.center-tag{
  position:absolute;
  bottom:2rpx;
  left:326rpx
}

.bottom-text{
  display:block;
  text-align:center;
  color:#7c7c7a;
  font-size:0.65rem;
}